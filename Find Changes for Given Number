#include <bits/stdc++.h>
using namespace std;
int deno[]={1,2,5,10,20,50,100,500,1000};
int size=sizeof(deno)/sizeof(deno[0]);
void find(int n)
{
    sort(deno,deno+size);
    vector<int> ans;
    for(int i=size-1;i>=0;i--)
    {
        while(n>=deno[i])
        {
            n-=deno[i];
            ans.push_back(deno[i]);
        }
    }
    for(int i=0;i<ans.size();i++)
    {
        cout<<ans[i]<<" ";
    }
}
int main()
{
    int n;
    cout<<"Enter=";
    cin>>n;
    cout<<"\nNumber of change for "<<n<<" :";
    find(n);
}
